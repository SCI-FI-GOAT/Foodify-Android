<?xml version="1.0" encoding="utf-8"?>
<android.support.v4.widget.DrawerLayout android:id="@+id/drawer_layout_admin"
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:fitsSystemWindows="true"
    tools:context=".AdminHomeActivity"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    tools:openDrawer="start">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="horizontal">

        <android.support.design.widget.NavigationView
            android:id="@+id/navig_view_admin"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            app:headerLayout="@layout/navig_header_admin"
            app:menu="@menu/home_menu_admin"/>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical">


            <FrameLayout
                android:id="@+id/fragment_container_up"
                android:layout_width="match_parent"
                android:layout_height="176dp" />

            <FrameLayout
                android:id="@+id/fragment_container_bottom"
                android:layout_width="match_parent"
                android:layout_height="match_parent" />


        </LinearLayout>

    </LinearLayout>

</android.support.v4.widget.DrawerLayout>